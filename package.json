{
  "name": "next_money",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "check-offline": "node check-offline.js",
    "check-react": "node check-react-version.js",
    "check-pwa": "node check-pwa.js",
    "check-node": "node check-node.js",
    "check-sw": "node check-sw.js",
    "verify": "npm run check-node && npm run check-pwa && npm run check-sw && npm run check-offline && npm run check-react",
    "prebuild": "npm run check-node && npm run check-pwa && npm run check-react",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "build:webpack": "next build --webpack",
    "postbuild": "npm run check-sw && npm run check-offline",
    "start": "next start",
    "lint": "eslint",
    "safe-update": "npm install && npm update && npm audit fix && npm run build && npm run verify",
    "clean-node": "npx rimraf node_modules package-lock.json && (npm cache clean --force || echo '⚠️  Cache lock ignored') && npm install",
    "clean-public": "npx rimraf ./public/sw.js ./public/fallback-*.js ./public/workbox-*.js ./public/_next/static/workbox/",
    "clean": "npm run clean-node && npm run clean-public"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "next": "^16.0.0",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.19.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "1.0.0",
    "chalk": "^5.6.2",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "semver": "^7.7.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
