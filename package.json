{
  "name": "next_money",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "check-react": "node check-react-version.js",
    "verify": "node check-node.js && node check-pwa.js && node check-sw.js",
    "prebuild": "node check-pwa.js && npm run check-react",
    "build": "next build",
    "postbuild": "node check-sw.js",
    "start": "next start",
    "lint": "eslint",
    "safe-update": "node check-node.js && npx rimraf node_modules package-lock.json && (npm cache clean --force || echo '⚠️  Cache lock ignored') && npm install && npm update && npm audit fix && npm run build"
  },
  "dependencies": {
    "next": "16.0.0",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "1.0.0",
    "chalk": "^5.6.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "semver": "^7.7.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
